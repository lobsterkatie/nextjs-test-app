{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "run `next dev`",
      "type": "pwa-node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": ["--preserve-symlinks", "--trace-warnings"],
      "program": "${workspaceFolder}/node_modules/.bin/next",
      "args": ["dev", "-p", "3001"],
      "outFiles": [
        "${workspaceFolder}/**/*.js",
        "!${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/node_modules/@sentry/**/*.js",
        "${workspaceFolder}/node_modules/next/**/*.js",
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**",
        "${workspaceFolder}/node_modules/next/dist/**",
        "${workspaceFolder}/node_modules/@sentry/*/dist/**"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "smartStep": false
    },
    {
      "name": "run `next start`",
      "type": "pwa-node",
      "preLaunchTask": "yarn build",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/tslib/**/*",
        "**/syncpromise.js"
      ],
      "runtimeArgs": ["--preserve-symlinks", "--trace-warnings"],
      "program": "${workspaceFolder}/node_modules/.bin/next",
      "args": ["start", "-p", "3001"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/**/*.js",
        "!${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/node_modules/@sentry/**/*.js",
        "${workspaceFolder}/node_modules/next/**/*.js",
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**",
        "${workspaceFolder}/node_modules/next/**",
        "${workspaceFolder}/node_modules/@sentry/*/dist/**/*"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      // "trace": true
    },
    {
      "name": "run `next build`",
      "type": "pwa-node",
      "request": "launch",
      "skipFiles": [
        // "<node_internals>/**",
        "${workspaceFolder}/node_modules/tslib/**"
      ],
      "smartStep": false,
      "runtimeArgs": ["--preserve-symlinks", "--trace-warnings"],
      "program": "${workspaceFolder}/node_modules/.bin/next",
      "args": ["build"],
      "outFiles": [
        "${workspaceFolder}/**/*.js",
        "!${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/node_modules/@sentry/**/*.js",
        "${workspaceFolder}/node_modules/next/**/*.js",
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**",
        "${workspaceFolder}/node_modules/next/dist/**",
        "${workspaceFolder}/node_modules/@sentry/*/dist/**"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      // "outputCapture": "std"
    }
  ]
}
