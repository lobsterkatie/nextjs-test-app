{
  "compilerOptions": {
    // "target": "esnext",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    // "skipLibCheck": false,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    // "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/[vegetable]/[nestedVeggieParam]/yo.js", "pages/meh/[rockParam]/bare.js", "pages/meh/hola.js", "pages/things.js"  ],
  "exclude": [
    "node_modules",
    "sentry-javascript"
  ]
}
